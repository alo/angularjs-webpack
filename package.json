    "css-loader": "^0.23.1",
    "http-server": "^0.8.5",
    "ng-annotate-loader": "0.0.10",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "angular": "^1.5.8"
  }
