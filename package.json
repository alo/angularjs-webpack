{
  "name": "webpack-angular",
  "version": "1.0.0",
  "description": "Angular with webpack tests",
  "repository": {
    "type": "git",
    "url": "https://github.com/alo/angularjs-webpack.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
